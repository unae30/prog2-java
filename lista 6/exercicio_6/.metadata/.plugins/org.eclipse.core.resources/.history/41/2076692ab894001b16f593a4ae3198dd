package veiculos;

public class Bicicleta extends Veiculo {
	
	private float velocidade;
	
	//get e set
	public float getVelocidade() {
		return velocidade;
	}

	//metodos abstratos


	public float acelerar(float velocidade) {
		this.velocidade = velocidade;
		return velocidade;
	}


	public void parar() {
		this.velocidade = 0;	
	}
	


}
