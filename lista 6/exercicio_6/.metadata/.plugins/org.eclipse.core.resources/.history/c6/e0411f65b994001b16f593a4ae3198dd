package veiculos;

public class Automóvel extends Veiculo {

	private float velocidade;
	
	//get 
	public float getVelocidade() {
		return velocidade;
	}
	@Override
	public float acelerar(float velocidade) {
		this.velocidade = velocidade;
		return velocidade;
	}

	@Override
	public void parar() {
		this.velocidade = 0;

	}

}
