package exe1;

public class ListaDeNomes {
	private String nome;

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
	
	@Override
	public boolean equals(Object obj) {
		ListaDeNomes p = (ListaDeNomes)obj;
		if(this.getNome().equals(p.getNome()))
			return true;
		else 
			return false;
	}
	
}
