package exe2;

public class Empregado {
	
	private String ID;
	private String nome;
	
	public Empregado(String ID, String nome) {
		this.ID = ID;
		this.nome = nome;
	}
	
	//criei pra fazer o teste da letra a) e não consigo mapear um objeto sem o atributo definido;
//	public Empregado() {};
	public String getID() {
		return ID;
	}
	public void setID(String iD) {
		ID = iD;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	

}
