package exe2;

import java.util.HashMap;
import java.util.Map;
import java.util.Set;

public class TesteMap {

	public static void main(String[] args) {
		
		Map<String, String> map = new HashMap<String, String>();
		
		Empregado e1 = new Empregado ("a1234", "Steve Jobs");
		Empregado e2 = new Empregado ("a1235", "Scott McNealy");
		Empregado e3 = new Empregado ("a1236", "Jeff Bezos");
		Empregado e4 = new Empregado ("a1237", "Larry Ellison");
		Empregado e5 = new Empregado ("a1238", "Bill Gates");
		
		map.put(e1.getID(), e1.getNome());
		map.put(e2.getID(), e2.getNome());
		map.put(e3.getID(), e3.getNome());
		map.put(e4.getID(), e4.getNome());
		map.put(e5.getID(), e5.getNome());
		
		Set<String> chaves = map.keySet();
		for(String chave: chaves) {
			System.out.println("ID: " + chave + "\r\n" + "Nome: " + map.get(chave) );
		}

		
	
		

	}

}
