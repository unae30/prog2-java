package exe8;

public class Controlador {

	public static void main(String[] args) {
		Divisao divisao = new Divisao();
		
		try {
			divisao.setNum1(10);
			divisao.setNum2(0);
			System.out.println(divisao.resultado());
		}catch(ArithmeticException a) {
			System.out.println("O denominador nao pode ser 0.");
		}
	}
	
}
