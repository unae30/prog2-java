package lanchonete;

public class SanduicheLanchoneteRT extends Sanduiches {

private String detalhesDosIngredientes; 
	
	protected SanduicheLanchoneteRT(String detalhes) {
		super("Lanchonete RT");
		this.detalhesDosIngredientes = detalhes;
	}
	
	@Override
	public String toString() {
		return super.toString() + " - " + this.detalhesDosIngredientes;
	}
}
