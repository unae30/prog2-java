package lanchonete;

public class SanduicheLanchoneteJP extends Sanduiches {

	private String detalhesDosIngredientes; 
	
	protected SanduicheLanchoneteJP(String detalhes) {
		super("Lanchonete JP");
		this.detalhesDosIngredientes = detalhes;
	}
	
	@Override
	public String toString() {
		return super.toString() + " - " + this.detalhesDosIngredientes;
	}
}
