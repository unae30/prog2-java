package lanchonete;

public class LanchoneteCG extends FabricaAbstrataDeSanduiches {

	@Override
	public Sanduiches FactorySanduiches(String nomeDaLanchonete) {
		if(nomeDaLanchonete.equals("Lanchonete CG"))
			return new Sanduiches("DETALHES: Pão integral, Queijo prato, Presunto de frango, Salada sem verdura");
		else if(nomeDaLanchonete.equals("Lanchonete JP"))
			return new Sanduiches("DETALHES: Pão francês, Queijo mussarela, Presunto de frango, Salada com verdura");
		else if(nomeDaLanchonete.equals("Lanchonete RT"))
			return new Sanduiches("DETALHES: Pão bola, Queijo cheddar, Presunto de peru, Salada sem verdura");
		else
			return new Sanduiches(nomeDaLanchonete);
	}
	


}
