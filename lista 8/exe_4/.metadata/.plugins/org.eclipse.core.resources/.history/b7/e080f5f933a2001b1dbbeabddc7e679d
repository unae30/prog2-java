package lanchonete;

public class SanduicheLanchoneteCG extends Sanduiches {
	
	private String detalhesDosIngredientes; 
	
	protected SanduicheLanchoneteCG(String nome, String detalhes) {
		super("Lanchonete CG");
		this.detalhesDosIngredientes = detalhes;
	}
	
	@Override
	public String toString() {
		return super.toString() + " - " + this.detalhesDosIngredientes;
	}
}
