package sandubao;

public class Main {

	public static void main(String[] args) {
		
		FabricaAbstrataDeSanduiche sandubao = Lanchonete.GetFactory("Sanduiches");	
		ISanduiches sanduiche = sandubao.getSanduiches("Lanchonete CG");
		sanduiche.ingredientes();
		
		PaoFactory pao = sandubao.getPao("Integral")
		pao.tipo();
		
		Presunto presunto = sandubao.getPresunto("Frango");
		presunto.tipo();
		
		Queijo queijo = sandubao.getQueijo("Mussarela");
		queijo.tipo();
		
		Salada salada = sandubao.getSalada("Sem verdura");
		salada.tipo();
		

	}

}
