package sandubao;

public class Lanchonete extends FabricaAbstrataDeSanduiches{

	@Override
	public ISanduichesIngredientFactory FactorySanduiche(String nomeLanchonete) {
		if(nomeLanchonete == null) 
			return null;
		else if(nomeLanchonete.equalsIgnoreCase("Lanchonete CG"))
			return new SanduichesIngredientCG();
		else if(nomeLanchonete.equalsIgnoreCase("Lanchonete CG"))
			return new SanduichesIngredientCG();
		else if(nomeLanchonete.equalsIgnoreCase("Lanchonete CG"))
			return new SanduichesIngredientCG();
		else
			return new ISanduichesIngredientFactory(nomeLanchonete);
	}


	
}
